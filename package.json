{
  "name": "quickcut-video-editor",
  "version": "1.0.0",
  "description": "A simple and easy-to-use online video editing tool for quick content sharing",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
